    alternate-background-color: rgba(255, 255, 255, 0.03);
}

/* Focus */
QTableWidget:focus {
    outline: none;
}

/* Header horizontal */
QHeaderView::section {
    background-color: rgba(255, 255, 255, 0.10); /* gris translucide uniforme */
    color: #e5e7ef;
    padding: 6px;
    border: none;
    border-right: 1px solid rgba(255, 255, 255, 0.07);
    font-weight: bold;
}

/* Dernière colonne sans bordure */
QHeaderView::section:last {
    border-right: none;
}

/* Header vertical */
QHeaderView::section:vertical {
    background-color: rgba(255, 255, 255, 0.10);
}

/* Hover */
QTableWidget::item:hover {
    background-color: rgba(255, 255, 255, 0.08); /* pas blanc, mais gris glacé */
}

/* Sélection */
QTableWidget::item:selected {
    background-color: rgba(120, 150, 255, 0.25); /* bleu givré */
    color: white;
}

/* Scrollbar */
QScrollBar:vertical {
    background: transparent;
    width: 10px;
    margin: 2px;
}

QScrollBar::handle:vertical {
    background: rgba(255, 255, 255, 0.20);
    border-radius: 5px;
}

QScrollBar::handle:vertical:hover {
    background: rgba(255, 255, 255, 0.30);
}

QScrollBar::add-line,
QScrollBar::sub-line {
    height: 0px;
}

QScrollBar::add-page,
QScrollBar::sub-page {
    background: none;
}
